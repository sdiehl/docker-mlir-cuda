{
    "name": "MLIR CUDA Dev Container",
    "build": { "dockerfile": "Dockerfile" },
    "initializeCommand": "sh .devcontainer/setup.sh",
    "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python"
    ],
    "forwardPorts": [
        8080
    ],
    "containerEnv": {
        "GITHUB_TOKEN": "${localEnv:GITHUB_TOKEN}",
        "GITHUB_USER": "${localEnv:GITHUB_USER}"
    }
}

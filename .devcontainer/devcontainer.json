{
    "name": "MLIR CUDA Dev Container",
    "image": "ghcr.io/sdiehl/docker-mlir-cuda:main",
    "initializeCommand": "sh .devcontainer/setup.sh",
    "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python"
    ],
    "forwardPorts": [
        8080
    ],
    "containerEnv": {
        "GITHUB_TOKEN": "${localEnv:GITHUB_TOKEN}"
    },
}